                                    "password",
                                    "roles"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Staff User"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "staff@pos.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "123456"
                                    },
                                    "roles": {
                                        "type": "array",
                                        "items": {
                                            "type": "string",
                                            "example": "STAFF"
                                        }
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "User created"
                    },
                    "401": {
                        "description": "Unauthenticated"
